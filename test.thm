<?xml version="1.0"?>
<alloy>

<view>

<defaultnode/>

<defaultedge/>

<node>
   <type name="Cargo"/>
   <type name="DriverFront"/>
   <type name="Int"/>
   <type name="OtherCargo"/>
   <type name="PassengerFront"/>
   <type name="PassengerSide"/>
   <type name="People"/>
   <type name="Rear"/>
   <type name="RearCargo"/>
   <type name="String"/>
   <type name="Transit"/>
   <type name="univ"/>
   <type name="seq/Int"/>
</node>

<node shape="Dbl Octagon">
   <type name="Vehicle"/>
</node>

<node style="Dotted" shape="Ellipse" color="Blue">
   <type name="Zone"/>
</node>

<node style="Solid" shape="Octagon" color="Yellow">
   <type name="Location"/>
</node>

<node visible="yes" color="Gray">
   <type name="Skip"/>
</node>

<node visible="yes" color="Green">
   <type name="Unlock"/>
</node>

<node visible="yes" color="Red">
   <type name="Lock"/>
</node>

<node visible="yes" shape="House" color="Blue">
   <type name="Command"/>
</node>

<node visible="yes" showlabel="no" shape="Inv House">
   <set name="$commands" type="Command"/>
</node>

<node visible="yes" showlabel="no" shape="Inv House" color="Gray">
   <set name="$skip" type="Command"/>
</node>

<edge color="Blue" style="Bold" visible="no">
   <relation name="zones"> <type name="Vehicle"/> <type name="Zone"/> </relation>
</edge>

<edge color="Green">
   <relation name="unlocked"> <type name="Vehicle"/> <type name="Zone"/> </relation>
</edge>

<edge color="Green" style="Bold" visible="yes">
   <relation name="$unlock"> <type name="Command"/> <type name="Vehicle"/> <type name="Zone"/> </relation>
</edge>

<edge color="Red">
   <relation name="locked"> <type name="Vehicle"/> <type name="Zone"/> </relation>
</edge>

<edge color="Red" style="Bold" visible="yes">
   <relation name="$lock"> <type name="Command"/> <type name="Vehicle"/> <type name="Zone"/> </relation>
</edge>

<edge visible="no">
   <relation name="locations"> <type name="Vehicle"/> <type name="Location"/> </relation>
   <relation name="locationsInZone"> <type name="Vehicle"/> <type name="Zone"/> <type name="Location"/> </relation>
</edge>

</view>

</alloy>
